generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model item {
  id          Int      @id @default(autoincrement())
  name        String?
  description String?
  category    String?
  price       Decimal? @db.Decimal
  quantity    Int?
}
